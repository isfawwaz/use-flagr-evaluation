{
  "name": "use-flagr-evaluation",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:repo": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:repo": "jest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.11",
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.js": "turbo run lint -- --ignore-pattern **/test"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@isfawwaz/eslint-config-base": "^1.0.0-rc.2",
    "@isfawwaz/eslint-config-jest": "^1.0.0-rc.2",
    "@isfawwaz/jest-config-base": "^1.0.0-rc.2",
    "@isfawwaz/prettier-config": "^1.0.0-rc-2",
    "@isfawwaz/tsconfig": "^1.0.0-rc.1",
    "check-dependency-version-consistency": "^3.0.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^5.0.2"
  },
  "dependencies": {}
}
